{
  "presets": [
    ["@babel/preset-react", {
      "pragma": "createElement"
    }],
    ["@babel/preset-env", {
       "targets": {
         "browsers": ["> 0.3%"]
      }
    }]
  ],
  "plugins": [
    "angularjs-annotate",
    "transform-async-to-promises"
  ],
  "ignore": ["**/vendor/*"],
  "env": {
    "development": {
      "presets": [
        ["@babel/preset-react", {
          "development": true,
          "pragma": "createElement"
        }]
      ]
    }
  }
}
